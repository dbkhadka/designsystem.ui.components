{
  "name": "designsystem.ui.components",
  "version": "0.0.1",
  "private": false,
  "main": "./dist/componentsLib.common.js",
  "scripts": {
    "build": "vue-cli-service storybook:build -c config/storybook",
    "serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "build-lib": "vue-cli-service build --target lib --name componentsLib ./src/main.js"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@mdx-js/react": "^2.1.2",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/vue": "^6.0.26",
    "@vue/cli-plugin-babel": "~4.5.7",
    "@vue/cli-service": "~4.5.7",
    "@vue/eslint-config-standard": "^5.1.2",
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "postcss": "^7.0.39",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "vue-cli-plugin-storybook": "~2.1.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "description": "Allied Electronics - Design System Components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbkhadka/designsystem.ui.components.git"
  },
  "keywords": [
    "Design",
    "System",
    "Components",
    "By",
    "using",
    "Vue2",
    "Tailwindcss",
    "and",
    "Storybook"
  ],
  "author": "Allied Electronics & Automation",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dbkhadka/designsystem.ui.components/issues"
  },
  "homepage": "https://github.com/dbkhadka/designsystem.ui.components#readme"
}
